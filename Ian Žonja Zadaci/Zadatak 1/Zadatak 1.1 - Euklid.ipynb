   "source": [
    "def rem_g(a,b):\n",
    "    o=a%b\n",
    "    while o!=0:\n",
    "        o=a%b\n",
    "        k = (a-o)/b\n",
    "        if o==0:\n",
    "            yield a, '=', int(k), \"*\", b, \"+\", o\n",
    "        else:\n",
    "            yield a, '=', b, \"*\", int(k), \"+\", o\n",
    "            a,b = b,o"
   ]
  },
